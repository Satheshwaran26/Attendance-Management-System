{
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": {
        "Cache-Control": "s-maxage=1, stale-while-revalidate"
      }
    }
  ],
  "rewrites": [
    { 
      "source": "/((?!api/.*).*)",
      "destination": "/index.html"
    }
  ]
}
